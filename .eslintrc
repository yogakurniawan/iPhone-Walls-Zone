{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "semi": 0,
    "template-tag-spacing": 0,
    "react/prop-types": 0,
    "comma-dangle": 0,
    "react/jsx-filename-extension": 0,
    "arrow-parens": 0,
    "class-methods-use-this": 0,
    "no-underscore-dangle": 0
  },
  "env": {
    "browser": true,
    "node": true
  }
}